{
  "name": "landing-page",
  "version": "1.0.0",
  "description": "Landing page for metatube project.",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint src/**/**.js",
    "pretty": "npx prettier-eslint --write src/**/**.js",
    "bundle": "./node_modules/.bin/webpack --config webpack.config.js",
    "build": "npm run lint && npm run pretty && npm run bundle",
    "app:dev": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --output-public-path /dist/ --port 3000",
    "app:prod": "./node_modules/.bin/webpack-dev-server --config webpack.config.js --output-public-path /dist/ --host 0.0.0.0 --port 8080 --no-live-reload",
    "start": "npm run app:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metatube/landing-page.git"
  },
  "author": "John Nolette<john@neetgroup.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metatube/landing-page/issues"
  },
  "homepage": "https://github.com/metatube/landing-page#readme",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@riotjs/compiler": "^4.1.1",
    "@riotjs/hot-reload": "^4.0.0",
    "@riotjs/webpack-loader": "^4.0.0",
    "animate.css": "^3.7.2",
    "animejs": "^3.0.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "d3": "^5.9.7",
    "file-loader": "^3.0.1",
    "firebase": "^6.3.0",
    "foobar-ipsum": "^1.0.3",
    "fullpage.js": "^3.0.5",
    "line-awesome": "^1.0.2",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "pretty-checkbox": "^3.0.3",
